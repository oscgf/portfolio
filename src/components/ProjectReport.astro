---
import File from "./icons/File.astro";

const { href } = Astro.props;
---

<a href={href}
    class="not-prose text-gray-900 bg-white border border-gray-300 focus:outline-none hover:bg-gray-100 focus:ring-4 focus:ring-gray-100 font-medium rounded-lg text-sm px-5 py-2.5 me-2 my-4 inline-flex items-center gap-x-2"
    target="_blank"
    rel="noopener noreferrer"
>
    <File class="size-5"/>
    Project Report
</a>